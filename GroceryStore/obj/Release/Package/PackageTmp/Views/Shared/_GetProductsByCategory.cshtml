
@{
    IEnumerable<GroceryStore.Models.Product> products = ViewBag.Products;
}

@foreach (GroceryStore.Models.Product product in products)
{
    <div class="divProduct">
        <span>@product.Product_price</span>
        <div>@product.product_name</div>
    </div>
}


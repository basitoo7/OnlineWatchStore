            $(document).on("click", ".proceedToCheckOut", function () {
                var orderTableTrs = [];
                var cartProductList = [];
                orderTableTrs = $(".yourordertable tbody.tbodyRow tr").toArray();
                var isQtyMissed = false;
                for (var i = 0; i < orderTableTrs.length; i++) {
                    var qty = $(orderTableTrs[i]).find("input.txtQty").val();
                    if (qty == "" || qty == "0" || qty == "00" || qty == "000" || isNaN(qty)) {
                        isQtyMissed = true;

                    }
                    //else{
                    //   qtyNotEmpty = qty;
                    //}
                    //return;
                }

                if ($(orderTableTrs).length == 0) {
                    alert("cart is empty, first add at least one product to cart, than proceed to checkout")
                    return;
                }

                else if (isQtyMissed) {
                    alert("Please enter valid quantity in the missing quantity field before checkout");
                    return;
                }

                else {
                    for (var i = 0; i < orderTableTrs.length; i++) {
                        $.grep(Categories.Structure.allProductListFromDB, function (item, index) {
                            if (orderTableTrs[i].id == item.product_id) {
                                var priceText = $(orderTableTrs[i]).find("td.rowTotal").text();
                                var priceValue = parseFloat(priceText);
                                item.Product_price = $(orderTableTrs[i]).find("td.unitPrice").text();
                                item.quantity = parseFloat($(orderTableTrs[i]).find("input.txtQty").val());
                                item["TotalPrice"] = $(orderTableTrs[i]).find("td.rowTotal").text();
                                item.TotalPrice = item.TotalPrice;
                                cartProductList.push(item);
                            }
                        });
                    }
                    Categories.Structure.Customer.CartProductList = cartProductList;
                    localStorage.setItem("customer", JSON.stringify(Categories.Structure.Customer));
                    var Customer = JSON.parse(localStorage.getItem("customer"));
                    location.href = '@Url.Action("Index", "CheckOut")';
                }
            });

        });
    </script>




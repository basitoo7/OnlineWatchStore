            $(document).on("change", ".yourordertable input.yourorderquantity", function () {
                var qtyValue = 0;
                var productQtyText = $(this).val();
                //var productQtyText = "";
                //if ($(this).hasOwnProperty('oninput')) {
                //    $(this).on('input', function (event) {
                //        productQtyText = this.value.replace(/[^0-9\\]+/g, '');
                //    });
                //}
                //else {
                //    $(this).on('input', function (event) {
                //        productQtyText = this.value = this.value.replace(/[^0-9\\]+/g, '');
                //}

                if (productQtyText == "" || productQtyText == "." || productQtyText.indexOf("..") != -1) {
                    //Categories.Common.showNotificationDialog("Please enter the missed quantity.", Categories.Common.notificationTypes.WARNING, {
                    //    buttons: {
                    //        Yes: {
                    //            text: "Ok",
                    //            class: "dialog_yesBtn",
                    //            click: function () {
                    //                $(this).dialog('close');

                    //            }
                    //        }
                    //    }
                    //});
                    $(this).css("border-color", "red");
                    //$(".warning_dialogBox").css("z-index", 9999);
                    alert("Please enter a valid quantity");
                    return;
                }

                    //var parentTable = $(this).parent().parent().parent().parent();
                    //var productDivData = $(productDiv).data("cartBoxDivData");

                else {
                    $(this).css("border-color", "");
                    qtyValue += parseFloat(productQtyText);
                    var unitPriceText = $(this).parent().prev("td").text();
                    var unitPriceValue = parseFloat(unitPriceText);

                    var productPrice = qtyValue * unitPriceValue;
                    var productPriceRoundedValue = productPrice.toFixed(2);

                    //$(productDiv).find("span.cartproductprice").text("Rs. " + productPriceRoundedValue).append(' <a class="cartproductRemove" href="javascript:void();">Remove</a>');
                    $(this).parent().next("td.rowTotal").text(productPriceRoundedValue);

                    Categories.Structure.calculateOrderTotal();

                }


            });


            $(document).on("cut copy paste", ".yourordertable input.yourorderquantity", function (e) {
                e.preventDefault();
            });



            $(document).on("click", ".productListCart", function () {
                //alert("cart button is clicked");
                //var buttonText = "close";
                //$(".ui-dialog-titlebar-close").html("close");
                var grandTotalText = $(".spnGrandTotal").text();
                if (grandTotalText == " " || grandTotalText == "0.00" || $(".spnGrandTotal").length == 0) {
                    $(".containerDivCart").hide();
                    alert("Cart is empty, please Add to cart at least one product");
                }
                else {
                    Categories.Structure.displayCartBox();
                    //how to scroll to specific div using jquery
                    $("html, body").animate({ scrollTop: $(".product_list_header").offset().top }, 400);
